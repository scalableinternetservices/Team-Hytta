// index.html.erb
<h1>Listing pictures</h1>

<div id="pictures">
  <%= render @pictures %>
</div>


// _picture.html.erb
<% div_for picture do %>
  <%= link_to image_tag(picture.thmb_url), picture.full_url %>

  <%= link_to 'Edit', edit_picture_path(picture) %>
  <%= link_to 'Destroy', picture, :confirm => 'Are you sure?', :method => :delete %>
<% end %>